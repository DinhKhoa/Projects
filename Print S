#include <iostream>
using namespace std;
int main(){
	int i,j,n,m;
	cout<<"enter n ";
	cin>>n;
	cout << endl;
	cout << "enter m ";
	cin >> m ;
	for(i=0;i<n;i++){
		for(j=0;j<m;j++){
     if(i==0 || i == int (n/2) || i == (n-1) || (j == 0 && i < int (n/2))  || (j == (m-1) && i > int (n/2)))
       cout<<"* ";
     else cout << "  ";}
    cout<<endl;}
     return 0;}
